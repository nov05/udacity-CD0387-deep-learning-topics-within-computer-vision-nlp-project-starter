{
    "Version": "2008-10-17",
    "Id": "Enforce HTTPS",
    "Statement": [
        {
            "Sid": "PublicReadAccessForPrefix",
            "Effect": "Allow",
            "Principal": "*",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::p3-dog-breed-image-classification/*"
        },
        {
            "Effect": "Allow",
            "Principal": "*",
            "Action": ["s3:PutObject"],
            "Resource": "arn:aws:s3:::p3-dog-breed-image-classification/*"
        }
    ]
}